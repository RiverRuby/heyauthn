// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id  String  @id
  groupId Int  
  reputation Int
  semaphorePublicKey String @unique
  timestamp String
  username String
}

model Message {
  id String @id 
  message String
  timestamp String
}